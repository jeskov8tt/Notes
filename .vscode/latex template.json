{
	"latex template": {
		"prefix": "tex",
		"body": [
			"\\documentclass{article}\r",
			"\\usepackage{graphicx}\r",
			"\\usepackage{amsmath}\r",
			"\\usepackage{amssymb}\r",
			"\\usepackage[italicdiff]{physics}\r",
			"\\usepackage{enumerate}\r",
			"\\usepackage{microtype}\r",
			"\\DisableLigatures{encoding= *, family=*}\r",
			"\\usepackage{titlesec}\r",
			"\\usepackage{xfrac}\r",
			"\\setcounter{secnumdepth}{4}\r",
			"\r",
			"\\titleformat{\\paragraph}\r",
			"{\\normalfont\\normalsize\\bfseries}{\\theparagraph}{1em}{}\r",
			"\\titlespacing*{\\paragraph}\r",
			"{0pt}{3.25ex plus 1ex minus .2ex}{1.5ex plus .2ex}\r",
			"\r",
			"\\title{}\r",
			"\\author{}\r",
			"\\date{}\r",
			"\r",
			"\\begin{document}\r",
			"\\maketitle\r",
			"\r",
			"\\section{}\r",
			"\r",
			"\\end{document}"
		],
		"description": "description for latex template"
	}
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
}
